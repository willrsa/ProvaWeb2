// Função para selecionar 10 cores aleatórias
function selecionarCoresAleatorias(vetorCores) {
    const coresAleatorias = [];
    const totalCores = vetorCores.length;

    while (coresAleatorias.length < 10) {
        const indiceAleatorio = Math.floor(Math.random() * totalCores); // Escolhe um índice aleatório
        const corSelecionada = vetorCores[indiceAleatorio];

        // Garante que a mesma cor não seja selecionada mais de uma vez
        if (!coresAleatorias.includes(corSelecionada)) {
            coresAleatorias.push(corSelecionada);
        }
    }

    return coresAleatorias;
}

// Testa a função com o vetor de cores
const coresAleatorias = selecionarCoresAleatorias(vetorCores);

// Exibe as 10 cores selecionadas no console
console.log("10 cores aleatórias:", coresAleatorias);

